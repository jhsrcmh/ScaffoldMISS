  </div>
  <div class="field">
    <%= f.label :projinf %><br />
    <%= f.text_field :projinf %>
  </div>
  <div class="field">
    <%= f.label :security_id %><br />
    <%= f.number_field :security_id %>
  </div>
  <div class="field">
    <%= f.label :ifon %><br />
    <%= f.number_field :ifon %>
  </div>
<div class="field">
	<%= collection_select :user, :username, User.all, :id, :username%>
</div>
  <div class="field">
    <%= f.label :time %><br />
    <%= f.time_select :time %>
  </div>
  <div class="field">
    <%= f.label :remarkinf %><br />
    <%= f.text_area :remarkinf %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
